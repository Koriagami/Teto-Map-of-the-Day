{
  "name": "teto-bot",
  "version": "1.0.0",
  "description": "Discord bot: Teto Bot - submit osu! maps and vote",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "node prisma/check-migration.js && prisma migrate deploy && node src/index.js",
    "deploy-commands": "node src/deploy-commands.js",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:verify": "node src/verify-db.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.89",
    "@prisma/client": "^5.7.1",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "prisma": "^5.7.1"
  }
}
